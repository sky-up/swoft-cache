{
  "name": "swoft-fork/cache",
  "type": "library",
  "keywords": [
    "php",
    "swoole",
    "swoft",
    "psr-16",
    "simple-cache",
    "cache"
  ],
  "description": "Cache Component for Swoft",
  "license": "Apache-2.0",
  "require": {
    "psr/simple-cache": "^1.0.0",
    "swoft-fork/serialize": "~2.1.0",
    "swoft-fork/stdlib": "~2.1.0",
    "swoft-fork/swlib": "~2.1.0"
  },
  "autoload": {
    "psr-4": {
      "Swoft\\Cache\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SwoftTest\\Cache\\": "test/unit"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  },
  "suggest": {
    "swoft/redis": "provide redis cache adapter support"
  }
}
